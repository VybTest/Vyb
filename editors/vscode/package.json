{
  "name": "vyb",
  "displayName": "Vyb",
  "description": "Syntax highlighting for Vyb test files",
  "version": "0.1.0",
  "publisher": "vybtest",
  "repository": {
    "type": "git",
    "url": "https://github.com/VybTest/Vyb"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "vyb",
        "aliases": ["Vyb", "vyb"],
        "extensions": [".vyb", ".ts.vyb", ".js.vyb", ".py.vyb", ".lua.vyb", ".go.vyb", ".rb.vyb"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "vyb",
        "scopeName": "source.vyb",
        "path": "./syntaxes/vyb.tmLanguage.json"
      }
    ]
  }
}
